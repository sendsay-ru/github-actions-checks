{
  "jobs": [
    {
      "key": "checks",
      "steps": [
        {
          "key": "eslint",
          "name": "ESLint",
          "command": "yarn eslint"
        },
        {
          "key": "prettier",
          "name": "Prettier",
          "command": "yarn prettify"
        },
        {
          "key": "playwright",
          "name": "Playwright",
          "command": "yarn e2e"
        }
      ],
      "preCommand": "npm run playwright install --with-deps",
      "artifacts": ["playwright-report", "playwright-artifacts"]
    },
    {
      "key": "tests",
      "steps": [
        {
          "key": "unit",
          "name": "Unit",
          "command": "yarn unit"
        }
      ]
    }
  ],
  "showComments": false,
  "node": 16,
  "installCommand": "yarn install",
  "buildCommand": ""
}
